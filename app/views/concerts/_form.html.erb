<%= form_for @concert do |f| %>
	<ul>
		<li>
			<%= f.label :name %>
			<%= f.text_field :name %>
		</li>
		<br>
		<li>
			<%= f.label :date %>
			<%= f.date_field :date %>
		</li>
		<br>
		<li>
			<%= f.label :artist %>
			<%= f.text_field :artist %>
		</li>
		<br>
		<li>
			<%= f.label :venue %>
			<%= f.text_field :venue %>
		</li>
		<br>
		<li>
			<%= f.label :city %>
			<%= f.text_field :city %>
		</li>
		<br>
		<li>
			<%= f.label :ticketprice %>
			<%= f.text_field :ticketprice %>
		</li>
		<br>
		<li>
			<%= f.label :description %>
			<%= f.text_field :description %>
		</li>
		<br>
		<%= f.submit "Save" %>
		<%= link_to("Home", concerts_path) %>
		<%= link_to("Cancel", concerts_path) %>
<% end %>

